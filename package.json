    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^3.0.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@octokit/auth-app": "^6.0.0",
    "applicationinsights": "^2.7.0",
    "axios": "^1.4.0",
    "axios-retry": "^4.0.0",
    "dotenv": "^16.0.3",
